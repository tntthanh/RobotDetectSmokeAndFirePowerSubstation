<launch>
    <!-- <include file="$(find mobile_mini)/launch/robot.launch" /> -->

    <node pkg="tf" type="static_transform_publisher" name="odom_to_map" args=" 0.1 0.0 0.2 0.0 0.0 0.0 map odom 100" />

    <node pkg="tf" type="static_transform_publisher" name="laser_to_base_link" args=" 0.11 0.0 0.14 0.0 0.0 0.0 base_link laser 100" />

    <node pkg="tf" type="static_transform_publisher" name="imu_link_to_base_link" args=" 0.12 -0.05 0.0 0.0 0.0 0.0 base_link imu_frame 0" />    


    <!-- Run the ekf for map to odom config -->
    <node name="odom" pkg="mobile_mini" type="odom_node.py"/>  

    <!-- Run mpu -->
    <include file="$(find mpu9250-driver)/launch/mpu9250.launch"/>

    <!-- Run lidar -->
    <include file="$(find rplidar_ros)/launch/rplidar.launch"/>  
    <include file="$(find mobile_mini)/include/my_laser_filter.launch" /> 

    <!-- Run serial node -->
    <include file="$(find mobile_mini)/include/serial_node.launch"/>
    

    <!-- Run the map server -->
    <include file="$(find mobile_mini)/include/start_map_server.launch" />

    <!--- Run Move Base -->
    <include file="$(find my_move_base)/launch/my_move_base.launch" />

    <include file="$(find mobile_mini)/launch/start_navsat.launch" />

    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find mobile_mini)/rviz/config.rviz" />

</launch>
